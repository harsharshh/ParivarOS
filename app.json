{
  "expo": {
    "name": "parivaros",
    "slug": "parivaros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "parivaros",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.harsharsh.parivaros"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.harsharsh.parivaros"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-firebase-core",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8b6edc14-a3d0-436b-873a-94624ad4756e"
      },
      "firebase": {
        "apiKey": "AIzaSyDx1uvHR8HHRxcE6w_1oS-jlsGr-_6YAy8",
        "authDomain": "parivaros.firebaseapp.com",
        "projectId": "parivaros",
        "storageBucket": "parivaros.firebasestorage.app",
        "messagingSenderId": "283224509905",
        "appId": "1:283224509905:web:9f039432922c3a142924f0",
